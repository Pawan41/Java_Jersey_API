<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping >

  <class name="com.demo.rest.Zoo" table="zoo">
	    <id name="zooId" column="z_id">
	      <generator class="native"/>
	    </id>
	   <property name="zooLocation" column="z_location"/>
	    <property name="zooName" column="z_name"/>
	
  </class>
  
</hibernate-mapping>