<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

  <class name="com.demo.rest.Animal" table="animal">
	    <id name="animalId" column="a_id">
	      <generator class="native"/>
	    </id>
	   <property name="animalGender" column="Gender"/>
	   <property name="animalName" column="a_name"/>
	   <property name="animalZooName" column="zoo_name"/>
	
  </class>
</hibernate-mapping>